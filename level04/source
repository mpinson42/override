int main()
{
	char str[32];
	int pid = fork();        //0xac
	int other;
	memset(str, 0, 32);

	if(pid != 0)
	{	
		prctl(1,1)
		ptrace(0,0,0)
		puts("Give me some shellcode, k");
		gets(str);
	}
	else
	{
		while(1)
		{
			wait(&pid)
			if(wait(&pid) == 0x7f)
			{
				if(ptrace(3,pid,44) == 0xb)
				{
					puts("child is exiting...")
					return(0);
				}
				else
					break
			}
		}
		puts("no exec() for you");
		kill(pid, 9)
	}
	return(0)
}